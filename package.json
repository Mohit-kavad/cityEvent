{
  "name": "cityevents",
  "version": "1.0.0",
  "description": "event handler platform",
  "main": "app.js",
  "scripts": {
    "start": "nodemon dist/server.js",
    "build": "rm -rf dist && npx tsc -w",
    "build:prod": "rm -rf dist && npx tsc",
    "dev": "export NODE_ENV=development && nodemon --exec ts-node --esm src/server.ts",
    "migrate": "export NODE_ENV=development && npx sequelize-cli db:migrate --options-path=src/database/options.ts",
    "migrate:undo:all": "export NODE_ENV=development && sequelize db:migrate:undo:all --options-path=src/database/options.ts",
    "migrate:undo": "export NODE_ENV=development && sequelize db:migrate:undo --options-path=src/database/options.ts",
    "migrate:test": "export NODE_ENV=test && sequelize db:migrate --options-path=src/database/options.ts",
    "migrate:undo:all:test": "export NODE_ENV=test && sequelize db:migrate:undo:all --options-path=src/database/options.ts",
    "test": "export NODE_ENV=test && jest --detectOpenHandles --watchAll"
  },
  "author": "Mohit kavad",
  "license": "ISC",
  "dependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/chalk": "^2.2.0",
    "@types/date-and-time": "^0.13.0",
    "@types/express": "^4.17.17",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/morgan": "^1.9.4",
    "@types/node-cron": "^3.0.8",
    "@types/nodemailer": "^6.4.9",
    "@types/sequelize": "^4.28.15",
    "@types/supertest": "^2.0.12",
    "bcrypt": "^5.1.0",
    "chalk": "^5.3.0",
    "config": "^3.3.9",
    "date-and-time": "^3.0.2",
    "dotenv": "^16.3.1",
    "dotenv-expand": "^10.0.0",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.0",
    "morgan": "^1.10.0",
    "node-cron": "^3.0.2",
    "nodemailer": "^6.9.4",
    "nodemon": "^2.0.22",
    "path": "^0.12.7",
    "pg": "^8.11.0",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.32.0",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.0",
    "typescript": "^5.1.3"
  },
  "devDependencies": {
    "@types/jest": "^29.5.2",
    "@types/node": "^20.2.5",
    "sequelize-cli": "^6.6.1",
    "ts-node": "^10.9.1"
  }
}
